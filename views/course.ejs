<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>COURSE</h1>

    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Experience</th>
                <th>Achievement</th>
                <th>Duration</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(el => { %>
                <tr>
                    <td><%= el.name %></td>
                    <td><%= el.experience %></td>
                    <td><%= el.achievement %></td>
                    <td><%= el.duration %></td>
                    <td>
                        <a href="/course/form">
                            <button>Book</button>
                        </a>
                    </td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>